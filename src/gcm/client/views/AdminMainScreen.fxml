<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="431.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gcm.client.controllers.AdminMainScreenController">
    <children>
        <TabPane layoutX="14.0" layoutY="14.0" prefHeight="315.0" prefWidth="582.0" side="BOTTOM"
                 tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                 AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <tabs>
                <Tab text="View Cities and Maps">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
                    </content>
                </Tab>
                <Tab text="Worker">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <VBox layoutX="22.0" layoutY="9.0" prefHeight="277.0" prefWidth="100.0" spacing="10.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#createCity" prefHeight="25.0"
                                                prefWidth="93.0" text="Create  City"/>
                                        <Button minWidth="30.0" mnemonicParsing="false" onAction="#createMap"
                                                prefHeight="25.0" prefWidth="93.0" text="Create Map"/>
                                        <Button mnemonicParsing="false" onAction="#editMap" prefHeight="25.0"
                                                prefWidth="93.0" text="Edit Map"/>
                                        <Button mnemonicParsing="false" onAction="#getMap" prefHeight="25.0"
                                                prefWidth="93.0" text="Get Map"/>
                                    </children>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                </VBox>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="adminContentManagerTab" text="Content Manager">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Button layoutX="34.0" layoutY="41.0" mnemonicParsing="false" onAction="#changePrice"
                                        prefHeight="31.0" prefWidth="111.0" text="Change Price"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="adminCompanyManagerTab" text="Company Manager">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                            <children>
                                <Button layoutX="38.0" layoutY="47.0" mnemonicParsing="false" onAction="#Approve"
                                        text="Approve Price Changes"/>
                            </children>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
</AnchorPane>
