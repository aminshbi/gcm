<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="335.0" prefWidth="210.0" stylesheets="@../stylesheets/main.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gcm.client.controllers.AddCityController">
    <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="20.0" AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0">
         <children>
              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add City">
                  <font>
                      <Font size="24.0" />
                  </font>
              </Text>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <VBox fillWidth="false" prefHeight="200.0" prefWidth="100.0">
                     <children>
                          <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name" />
                          <TextField fx:id="namefield" prefHeight="27.0" prefWidth="173.0" />
                     </children>
                  </VBox>
                  <VBox fillWidth="false" prefHeight="200.0" prefWidth="100.0">
                     <children>
                          <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Country" />
                          <TextField fx:id="countryfield" prefWidth="173.0" />
                     </children>
                  </VBox>
                  <VBox fillWidth="false" prefHeight="200.0" prefWidth="100.0">
                     <children>
                          <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Purchase Price" />
                          <TextField fx:id="purchase_price_field" prefHeight="27.0" prefWidth="173.0" />
                     </children>
                  </VBox>
                  <VBox fillWidth="false" prefHeight="200.0" prefWidth="100.0">
                     <children>
                          <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Subscription Price" />
                          <TextField fx:id="subscription_price_field" prefWidth="173.0" />
                     </children>
                  </VBox>
                  <VBox alignment="BASELINE_LEFT" prefHeight="200.0" prefWidth="100.0">
                     <children>
                          <Button mnemonicParsing="false" onAction="#addCityToDB" prefHeight="27.0" prefWidth="64.0" text="Add" />
                     </children>
                  </VBox>
               </children>
            </VBox>
         </children>
      </VBox>
    </children>
</AnchorPane>
