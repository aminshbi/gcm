<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="569.0" prefWidth="539.0" stylesheets="@../stylesheets/main.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gcm.client.controllers.ApprovePriceController">
    <children>
        <ButtonBar layoutX="7.0" layoutY="168.0" prefHeight="40.0" prefWidth="187.0"/>
        <Text layoutX="14.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Approve Price Changing">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <Button layoutX="119.0" layoutY="85.0" mnemonicParsing="false" onAction="#getPrice" prefHeight="31.0"
                prefWidth="274.0" text="Get Price Changing Requests"/>
        <Button layoutX="15.0" layoutY="524.0" mnemonicParsing="false" onAction="#back" text="Back"/>
        <Button layoutX="35.0" layoutY="400.0" mnemonicParsing="false" onAction="#ApprovePrice" prefHeight="31.0"
                prefWidth="154.0" text="Approve Price"/>
        <TableView id="cities" fx:id="tableList" editable="true" layoutX="35.0" layoutY="125.0" prefHeight="213.0"
                   prefWidth="470.0">
            <columns>
                <TableColumn id="city" fx:id="city" prefWidth="75.0" text="City"/>
                <TableColumn id="country" fx:id="country" prefWidth="75.0" text="Country"/>
                <TableColumn id="old_purchase" fx:id="old_purchase" prefWidth="75.0" text="Old Purchase Price"/>
                <TableColumn id="new_purchase" fx:id="new_purchase" prefWidth="75.0" text="New Purchase Price"/>
                <TableColumn id="old_sub" fx:id="old_sub" prefWidth="75.0" text="Old Subscription Price"/>
                <TableColumn id="new_sub" fx:id="new_sub" prefWidth="75.0" text="New Subscribtion Price"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
        <Button layoutX="351.0" layoutY="400.0" mnemonicParsing="false" onAction="#DeclinePrice" prefHeight="31.0"
                prefWidth="154.0" text="Decline Changing"/>
    </children>
</AnchorPane>
