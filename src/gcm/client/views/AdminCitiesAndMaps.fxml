<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>
<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="469.0" prefWidth="663.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gcm.client.controllers.AdminCitiesAndMapsController">
    <children>
        <ListView fx:id="citiesList" layoutX="14.0" layoutY="31.0" onMouseClicked="#citiesListSelectCity"
                  prefHeight="386.0" prefWidth="167.0">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ListView>
        <Text layoutX="14.0" layoutY="24.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cities"/>
        <Pane fx:id="cityInfoPane" layoutX="191.0" layoutY="10.0" prefHeight="413.0" prefWidth="459.0">
            <children>
                <ListView fx:id="cityMapsList" layoutX="230.0" layoutY="25.0" onMouseClicked="#cityMapsListSelectMap"
                          prefHeight="382.0" prefWidth="207.0">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                </ListView>
                <Text layoutX="230.0" layoutY="19.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Maps"/>
                <Label fx:id="cityInfoLabel" alignment="TOP_LEFT" layoutX="14.0" layoutY="23.0" prefHeight="367.0"
                       prefWidth="199.0" text="selected city info"/>
            </children>
        </Pane>
    </children>
</AnchorPane>
