<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="258.0" prefWidth="271.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="gcm.client.controllers.LoginController">
    <children>
        <TextField fx:id="usernameField" layoutX="14.0" layoutY="76.0" prefHeight="27.0" prefWidth="240.0"/>
        <PasswordField fx:id="passwordField" layoutX="14.0" layoutY="129.0" prefHeight="27.0" prefWidth="240.0"/>
        <ButtonBar layoutX="54.0" layoutY="160.0" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button minWidth="36.0" mnemonicParsing="false" onAction="#loginButtonClick" prefHeight="27.0"
                        text="Login"/>
            </buttons>
        </ButtonBar>
        <Text layoutX="14.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login">
            <font>
                <Font size="24.0"/>
            </font>
        </Text>
        <Text layoutX="14.0" layoutY="72.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username"/>
        <Text layoutX="15.0" layoutY="124.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password"/>
        <Hyperlink layoutX="14.0" layoutY="211.0" onAction="#openRegisterView" prefHeight="25.0" prefWidth="240.0"
                   text="Don't have an account yet? Register"/>
    </children>
</AnchorPane>
