<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="431.0" prefWidth="663.0" stylesheets="@../stylesheets/main.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gcm.client.controllers.SearchCityOrAttractionController">
    <children>
        <TextField fx:id="searchQueryTF" layoutX="14.0" layoutY="34.0" prefHeight="27.0" prefWidth="337.0"/>
        <Button layoutX="349.0" layoutY="34.0" mnemonicParsing="false" onAction="#searchButtonClick" text="Search"/>
        <ListView fx:id="citiesList" layoutX="15.0" layoutY="98.0" orientation="HORIZONTAL" prefHeight="137.0"
                  prefWidth="633.0"/>
        <Text layoutX="15.0" layoutY="87.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cities matching search"/>
        <ListView fx:id="attractionsList" layoutX="15.0" layoutY="280.0" orientation="HORIZONTAL" prefHeight="137.0"
                  prefWidth="633.0"/>
        <Text layoutX="15.0" layoutY="269.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Attractions matching search"/>
        <Text layoutX="15.0" layoutY="24.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Welcome! Start by searching for a city, country, or attraction."/>
    </children>
</AnchorPane>
